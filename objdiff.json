      "target_path": "expected/build/src/adpcm/iopadpcm.c.o",
      "base_path": "build/src/adpcm/iopadpcm.c.o",
      "metadata": {}
    },
    {
      "name": "src/adpcm/ia_func",
      "target_path": "expected/build/src/adpcm/ia_func.c.o",
      "base_path": "build/src/adpcm/ia_func.c.o",
      "metadata": {}
    },
    {
      "name": "src/adpcm/ia_cmd",
      "target_path": "expected/build/src/adpcm/ia_cmd.c.o",
      "base_path": "build/src/adpcm/ia_cmd.c.o",
      "metadata": {},
      "symbol_mappings": {
        "IAdpcmCmdStop": "IAdpcmCmdtop",
        "jtbl_0000B300": "[.rodata-0]"
      }
    },
    {
      "name": "src/cdvd/iopcdvd",
      "target_path": "expected/build/src/cdvd/iopcdvd.c.o",
      "base_path": "build/src/cdvd/iopcdvd.c.o",
